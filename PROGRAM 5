#include <stdio.h>
int main()
{
    int a;
    int b;
    int c;
    int d;
    int e;
    float VALUE;
    printf("Enter two numbers: ");
    scanf("%d %d", &a, &b);
    c = a + b;
    d = a - b;
    e = a * b;
    VALUE = (float)a / b;
    printf("c= %d\n", c);
    printf("d= %d\n", d);
    printf("e= %d\n", e);
    printf("VALUE= %f\n", VALUE);
    return 0;
}
